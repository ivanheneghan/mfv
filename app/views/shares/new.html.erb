<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Enter Your Share Details</h1>
  <%= simple_form_for @share do |f| %>
    <%= f.input :company %>
    <%= f.input :ticker %>    
    <%= f.input :volume %>    
    <br /> 
    <%= f.submit 'Create', class: 'btn btn-primary' %>
  <% end %>
</div>