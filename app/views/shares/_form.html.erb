<%= simple_form_for(@share) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :share_id %>
    <%= f.input :name %>
    <%= f.input :company %>
    <%= f.input :share_price %>
    <%= f.input :volume %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
